pipeline {
agent any

environment {
dotnet ='path\\to\\dotnet.exe'
}

stages {
stage ('Checkout'){
steps {
git url:'https://github.com/Savitha668/pipelines-dotnet-core', branch: 'master'
      }
}
stage ('Restore PACKAGES'){
steps{
bat 'dotnet restore'
}
}

stage ('Built'){
steps{
bat 'dotnet build --configuration Release'
}
}

stage ('Publish'){
steps{
bat 'dotnet Publish --configuration Release'
}
}

}
}
